<!--miniprogram/pages/buy/buy.wxml-->
<import src="../templates/carListThin/carListThin.wxml" />
<view class="buy-page">
	<view class="header">
		<searchbar isnavigator="true"/>
		<filtertabmodule />
	</view>
	<view class="main">
		<scroll-view class="main-scroll" scroll-y style="height: 100%">
			<view wx:for="{{carList}}" wx:key="id" class="buy-group">		
				<template is="carlist-thin" data="{{...item}}"></template>
			</view>
		</scroll-view>
	</view>
</view>
