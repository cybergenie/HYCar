<!--miniprogram/templates/carList-main/carList-main.wxml-->
<template name='carlist-main'>
<view class="car-list-group">	
		<image class="car-img" src="{{smallImage}}"/>
		<view class="car-info">
			<view class="car-name">{{modelName}}</view>
			<view class="car-detail">{{cityName}}| {{year}}| {{mileage}}公里</view>
			<view class="sale-price">{{price}}</view>
		</view>	
	</view>
</template>
